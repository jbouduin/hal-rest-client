{
  "name": "@jbouduin/hal-rest-client",
  "author": "Johan Bouduin",
  "contributors": [
    "Johan Bouduin",
    "Thomas Deblock (original library)"
  ],
  "version": "0.7.1",
  "description": "Hal rest client for typescript",
  "tags": [
    "HAL",
    "Rest",
    "Typescript",
    "Webservice",
    "HTTP"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbouduin/hal-rest-client.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "http://github.com/jbouduin/hal-rest-client/issues"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "uri-templates": "^0.2.0"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.23",
    "@types/uri-templates": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "btoa": "^1.2.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.7.0",
    "eslint-plugin-jsdoc": "^39.0.0",
    "jest": "^27.4.7",
    "nock": "^13.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "bump": "npm version -m \"Bumped to %s\"",
    "test": "jest --coverage",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "prepare": "npm run build",
    "preversion": "npm test && npm run lint",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "coveralls": "jest --coverage && coveralls < coverage/lcov.info",
    "precommit": "npm run test && npm run lint"
  }
}
